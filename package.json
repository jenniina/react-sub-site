{
  "name": "react-sub-site-jenniina-fi",
  "description": "React sub site for jenniina.fi",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "build:debug": "cross-env NODE_ENV=debug vite build",
    "build:prod": "cross-env NODE_ENV=production vite build",
    "build:preview": "cross-env ANALYZE=true vite build",
    "preview:a": "npm run build:preview && vite preview",
    "preview": "vite preview",
    "lint": "eslint . --ext .ts,.tsx",
    "lint:fix": "eslint . --ext .ts,.tsx --fix",
    "type-check": "tsc --noEmit",
    "format": "prettier --write \"src/**/*.{ts,tsx,json,css,md}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,json,css,md}\"",
    "format:lint": "npm run format && npm run lint:fix",
    "knip": "knip --reporter=json > reports/knip.json"
  },
  "author": "Jenniina Laine",
  "license": "Proprietary",
  "dependencies": {
    "@reduxjs/toolkit": "^2.0.1",
    "axios": "^1.5.1",
    "dom-to-image-more": "^3.5.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-helmet-async": "^2.0.5",
    "react-icons": "^5.4.0",
    "react-redux": "^8.1.3",
    "react-router-dom": "^6.30.1",
    "uuid": "^9.0.0"
  },
  "resolutions": {
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  },
  "devDependencies": {
    "@types/eslint-plugin-jsx-a11y": "^6.10.1",
    "@types/react": "^18.2.41",
    "@types/react-dom": "^18.0.8",
    "@types/uuid": "^8.3.4",
    "@vitejs/plugin-react": "^4.3.4",
    "cross-env": "^10.1.0",
    "esbuild": "^0.25.0",
    "eslint": "^9.39.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.1",
    "globals": "^16.5.0",
    "jiti": "^2.6.1",
    "knip": "^5.69.0",
    "prettier": "^3.6.2",
    "rollup-plugin-copy": "^3.5.0",
    "terser": "^5.44.1",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.46.4",
    "vike": "^0.4.245",
    "vite": "^6.0.0"
  }
