{
  "mimeTypes": {
    ".js": "text/javascript",
    ".mjs": "text/javascript",
    ".jsx": "text/javascript"
  },
  "navigationFallback": {
    "rewrite": "/index.html",
    "exclude": [
      "/assets/*",
      "*.{css,scss,js,png,gif,ico,jpg,svg,woff,woff2,ttf,eot}"
    ]
  },
  "routes": [
    {
      "route": "/assets/*.js",
      "headers": {
        "content-type": "application/javascript; charset=utf-8",
        "cache-control": "max-age=31536000"
      }
    },
    {
      "route": "/assets/*",
      "headers": {
        "cache-control": "max-age=31536000"
      }
    },
    {
      "route": "/*",
      "serve": "/index.html",
      "statusCode": 200
    }
  ],
  "responseOverrides": {
    "404": {
      "rewrite": "/index.html"
    }
  },
  "trailingSlash": "never"
}
