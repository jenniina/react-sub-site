{
  "mimeTypes": {
    ".js": "text/javascript",
    ".mjs": "text/javascript",
    ".jsx": "text/javascript"
  },
  "navigationFallback": {
    "rewrite": "/index.html",
    "exclude": ["/assets/*", "*.{css,scss,js,png,gif,ico,jpg,svg}"]
  },
  "routes": [
    {
      "route": "/assets/*",
      "headers": {
        "cache-control": "max-age=31536000",
        "content-type": "text/javascript"
      }
    },
    {
      "route": "/*",
      "serve": "/index.html",
      "statusCode": 200
    }
  ],
  "responseOverrides": {
    "404": {
      "rewrite": "/index.html"
    }
  }
}
